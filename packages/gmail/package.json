{
  "name": "@sowonai/mcp-gmail",
  "version": "0.2.0",
  "license": "MIT",
  "description": "A Model Context Protocol (MCP) server for Gmail integration",
  "main": "dist/main.js",
  "module": "dist/main.js",
  "bin": {
    "mcp-gmail": "dist/main.js"
  },
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "nest build",
    "start": "node dist/main.js",
    "dev": "dotenv -e .env.test -- nest start --watch -- --log",
    "debug": "dotenv -e .env.test -- nest start --debug --watch -- --log",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "cli:install": "npm run build && node dist/main.js --install",
    "cli:install:log": "npm run build && node dist/main.js --install --log",
    "prepack": "npm run build && echo '#!/usr/bin/env node' | cat - dist/main.js > temp && mv temp dist/main.js && chmod +x dist/main.js"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@nestjs/common": "^11.0.5",
    "@nestjs/core": "^11.0.5",
    "@nestjs/platform-express": "^11.0.5",
    "@sowonai/nestjs-google-oauth-integration": "^0.1.3",
    "@sowonai/nestjs-mcp-adapter": "file:../../../nestjs-mcp-adapter/sowonai-nestjs-mcp-adapter-0.1.0-dev.0.tgz",
    "googleapis": "^148.0.0",
    "mime": "^4.0.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "yargs": "^17.7.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.5",
    "@nestjs/testing": "^11.0.5",
    "@types/express": "^4.17.17",
    "@types/mime": "^3.0.4",
    "@types/node": "^20.3.1",
    "@types/yargs": "^17.0.33",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.2",
    "unplugin-swc": "^1.5.2",
    "vitest": "^3.1.3",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sowonlabs/mcp-servers.git"
  }
}
